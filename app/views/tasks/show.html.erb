<p id="notice"><%= notice %></p>

<p>
  <strong>タスク名:</strong>
  <%= @task.name %>
</p>

<p>
  <strong>Content:</strong>
  <%= @task.content %>
</p>

<p>
  <strong>終了期限:</strong>
  <%= @task.limit %>
</p>

<p>
  <strong>状態:</strong>
  <%= @task.status %>
</p>

<p>
  <strong>優先順位:</strong>
  <%= @task.priority %>
</p>

<p>
  <strong>ラベル:</strong>
  <% @task.labels.each do |label| %>
    <%=label.name%>
  <% end %>
</p>


<%= link_to 'Edit', edit_task_path(@task) %> |
<%= link_to 'Back', tasks_path %>
